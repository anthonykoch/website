{
  "name": "website",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "anthonykoch <anthonykoch@users.noreply.github.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development NUXT_HOST=0.0.0.0 nuxt",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production nuxt start",
    "gen": "rm -rf dist && cross-env NODE_ENV=production nuxt generate",
    "dev:staging": "http-server dist-staging -p 4000 -a 0.0.0.0 -g",
    "gen:staging": "rm -rf dist-staging && cross-env NODE_ENV=production STAGING=true nuxt generate && gzip -fk dist-staging/**/*.{js,css,png,jpg}",
    "deploy": "rm -rf dist && cross-env NODE_ENV=production nuxt generate && node deploy.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/google-analytics": "^2.0.2",
    "axios": "^0.18.0",
    "bladejs": "https://github.com/anthonykoch/bladejs/tarball/master",
    "cheerio": "^1.0.0-rc.2",
    "cuid": "2.1.1",
    "date-fns": "^2.0.0-alpha.8",
    "front-matter": "^2.3.0",
    "gh-pages": "^1.2.0",
    "highlight.js": "^9.12.0",
    "html-loader": "^0.5.5",
    "http-server": "^0.11.1",
    "image-size-loader": "https://github.com/boopathi/image-size-loader/tarball/master",
    "intersection-observer": "^0.5.1",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-named-headings": "^1.1.0",
    "nuxt": "^2.4.3",
    "sassdash": "^0.9.0",
    "schema-utils": "^1.0.0",
    "slugify": "^1.3.4",
    "vue-focus-lock": "^1.2.0",
    "vue-scrollto": "^2.13.0",
    "vue-svg-loader": "^0.12.0",
    "vuex-persistedstate": "^2.5.4",
    "waypoints": "https://github.com/summerisgone/waypoints/tarball/master"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "babel-eslint": "8.2.1",
    "cross-env": "5.1.5",
    "cssnano": "^4.1.4",
    "eslint": "4.15.0",
    "eslint-plugin-vue": "4.0.0",
    "glob": "^7.1.2",
    "node-sass": "^4.9.3",
    "sass-loader": "7.0.1",
    "vue-loader": "^15.6.2",
    "webpack-bundle-analyzer": "2.12.0"
  },
  "browserslist": [
    "last 2 versions",
    "Safari 6",
    "IE 10"
  ]
}
